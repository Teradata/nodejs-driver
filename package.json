{
  "name": "teradata-nodejsdriver",
  "version": "1.0.0-beta.1",
  "description": "Teradata Database Node.js Driver",
  "main": "teradata-connection.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "run-script-os": "^1.0.3",
    "teradata-nativelib-darwin": "^1.0.0-beta.1.1",
    "teradata-prebuilt-fastcall-darwin-node-v57": "^0.2.6-1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "chai": "^4.1.2",
    "glob": "^6.0.4",
    "gulp": "3",
    "gulp-bump": "2.1.0",
    "gulp-help": "1.6.1",
    "gulp-if": "2.0.1",
    "gulp-util": "3.0.7",
    "mocha": "^5.2.0",
    "require-dir": "0.3.2",
    "rimraf": "2.6.2",
    "rollup": "^0.35.9",
    "rollup-plugin-commonjs": "4.1.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "run-sequence": "^1.2.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.0.1",
    "typescript-formatter": "^7.2.2"
  },
  "scripts": {
    "test": "rimraf src/node_modules && gulp copy-native-lib-for-test && npm run build && mocha -r ts-node/register test/**/*.ts",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts ./examples/typescript/src/*.ts",
    "format": "tsfmt -r",
    "build": "run-script-os",
    "build:win32": "npm run clean && tsc",
    "build:darwin": "npm run clean && tsc",
    "build:linux": "npm run clean && tsc",
    "publish": "npm run build && gulp publish-nodejsdriver",
    "install": "node scripts/install-dependencies.js",
    "i": "node scripts/install-dependencies.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Teradata/nodejs-driver"
  },
  "keywords": [
    "node.js",
    "Typescript",
    "Teradata"
  ],
  "author": "Jeremy Smartt, Wei-Ti Huang",
  "license": "MIT"
}
